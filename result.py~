#coding=utf-8
from numpy import *

def readdata():
    fr = open("tms.result",'r')
    fo = open("resultTMS1.csv",'w+')
    arrayOfLines = fr.readlines()
    total = 0
    i = 800001
    for line in arrayOfLines:    
        line = line.strip()
        line = line.split('\t')
        
        if line[1] == '0.5':
            line[0] = '-1'
        if line[0] == '1':
            total += 1
        if line[0] == '-1':
            line[0] = '0'
        
        if line[2] != str(i):
            print i
            fo.write(str(i)+',0'+'\n')
            i += 1
        i += 1
        fo.write(line[2]+','+line[0]+'\n')
    print total   
    fr.close()
    fo.close()
    
if __name__ == "__main__":
    readdata()
